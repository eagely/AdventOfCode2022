fun main(args: Array<String>) {
    val input = ""
    var chars = input.substring(0, 4)
    var equalNums = 0
    var output = 0
    for(i in 3 until input.length) {
        for(j in 0..3) {
            for(k in 0..3) {
                if(chars[j] == chars[k] && j != k)
                    equalNums++
            }
        }
        if(equalNums == 0) {
            output = input.indexOf(chars)
            println("The first non repeating sequence is at ${output+4}")
            break
        }
        else
            equalNums = 0
        chars = chars.substring(1, 4)
        chars += input[i].toString()
    }
}
